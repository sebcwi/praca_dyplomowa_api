          "tags":["User"],
          "parameters":[{
            "in":"body",
            "name":"body",
            "description":"Register user",
            "required":true,
            "schema":{
              "$ref":"#/definitions/User"
            }
          }],
          "responses":{
            "200":{
              "description":"Success"
            },
            "404":{
              "description":"Not Found"
            }
          }
        }
      }
